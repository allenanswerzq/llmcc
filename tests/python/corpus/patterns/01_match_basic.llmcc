def process(value):
    match value:
        case 1:
            return "one"
        case 2:
            return "two"
        case _:
            return "other"

def match_string(text):
    match text:
        case "hello":
            return "greeting"
        case "bye":
            return "farewell"
        case _:
            return "unknown"

