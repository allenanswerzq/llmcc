from typing import TypeAlias, Union, List, Dict

# Simple type alias
UserId: TypeAlias = int
Username: TypeAlias = str

# Complex type alias
UserMap: TypeAlias = Dict[UserId, Username]
UserList: TypeAlias = List[UserId]

# Union type alias
StringOrInt: TypeAlias = Union[str, int]
OptionalStr: TypeAlias = str | None

class User:
    id: UserId
    name: Username

def get_users() -> UserList:
    return []

