class Calculator:
    value: int

    def __init__(self, initial: int = 0):
        self.value = initial

    def add(self, n: int) -> int:
        self.value += n
        return self.value

    def subtract(self, n: int) -> int:
        self.value -= n
        return self.value

    @staticmethod
    def zero() -> int:
        return 0

    @classmethod
    def from_string(cls, s: str):
        return cls(int(s))

    @property
    def current(self) -> int:
        return self.value

