def identity(x: int) -> int:
    return x

def add(a: int, b: int) -> int:
    return a + b

def concat(s1: str, s2: str) -> str:
    return s1 + s2

def optional_return(flag: bool) -> str | None:
    if flag:
        return "yes"
    return None

def no_return(msg: str) -> None:
    print(msg)

