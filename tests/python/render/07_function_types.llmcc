# Sample package

--- file: sample/functions.py ---
from typing import List, Dict, Optional

def no_annotations(x, y):
    return x + y

def with_types(x: int, y: int) -> int:
    return x + y

def complex_types(
    items: List[str],
    mapping: Dict[str, int]
) -> Optional[str]:
    return items[0] if items else None

def void_return(message: str) -> None:
    print(message)

