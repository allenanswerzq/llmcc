# Sample package

--- file: sample/enums.py ---
from enum import Enum

class Color(Enum):
    RED = "red"
    GREEN = "green"
    BLUE = "blue"

class Direction(Enum):
    NORTH = "N"
    SOUTH = "S"
    EAST = "E"
    WEST = "W"

lang: python

--- file: sample/graphics.py ---
from .enums import Color, Direction

def set_background(color: Color) -> None:
    pass

def get_primary_color() -> Color:
    return Color.RED

def move(direction: Direction, steps: int) -> tuple[int, int]:
    return (0, 0)

def get_opposite(direction: Direction) -> Direction:
    opposites = {
        Direction.NORTH: Direction.SOUTH,
        Direction.SOUTH: Direction.NORTH,
        Direction.EAST: Direction.WEST,
        Direction.WEST: Direction.EAST,
    }
    return opposites[direction]

