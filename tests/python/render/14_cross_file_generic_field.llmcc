from typing import List, Dict, Optional
from .types import User, Error

class UserRepository:
    users: List[User]
    errors: Dict[str, Error]
    current: Optional[User]

    def get_all(self) -> List[User]:
        return self.users

    def get_errors(self) -> Dict[str, Error]:
        return self.errors

