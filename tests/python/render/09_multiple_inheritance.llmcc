  )
  (class:6 Loggable
    (method:7 log)
  )
  (class:8 Document
    (field:9 content @type str)
    (@inherit:2 Serializable)
    (@inherit:4 Printable)
  )
  (class:10 Report
    (field:11 title @type str)
