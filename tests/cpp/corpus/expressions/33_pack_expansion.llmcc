Parameter pack expansion.
lang: cpp

--- file: src/lib.cpp ---
template<typename... Args>
void print(Args... args) {
    (void)sizeof...(args);
}

template<typename... Ts>
struct TypeList {
    static constexpr int size = sizeof...(Ts);
};

template<int... Is>
struct IndexList {
    static constexpr int sum = (Is + ...);
};

