template<typename T>
class Container {
    T value;
public:
    void set(T v) { value = v; }
    T get() const { return value; }
};

void test_templates() {
    Container<int> int_container;
    int_container.set(42);
    int val = int_container.get();

    Container<double> double_container;
    double_container.set(3.14);
}

