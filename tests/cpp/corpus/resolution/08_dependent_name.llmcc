===============================================================================
dependent-name-resolution
===============================================================================
Dependent name resolution in templates.
lang: cpp

--- file: src/lib.cpp ---
template<typename T>
struct Container {
    typename T::value_type value;

    void process() {
        T::static_method();
    }

    template<typename U>
    void convert() {
        typename U::result_type result;
    }
};

struct Data {
    using value_type = int;
    using result_type = double;
    static void static_method() {}
};

