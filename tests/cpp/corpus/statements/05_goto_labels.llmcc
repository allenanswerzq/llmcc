void process_matrix(int matrix[3][3]) {
    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            if (matrix[i][j] < 0)
                goto error;
        }
    }
    return;

error:
    return;
}

void multi_cleanup() {
    goto cleanup;

cleanup:
    return;
}

