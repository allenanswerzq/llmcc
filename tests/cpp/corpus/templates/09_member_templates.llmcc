class Container {
public:
    template<typename T>
    T convert() const { return T(); }

    template<typename U>
    void assign(U value) {}
};

template<typename T>
class GenericContainer {
public:
    template<typename U>
    U convert_to() const { return U(); }

    template<typename U>
    bool compare(const GenericContainer<U>& other) const { return true; }
};

