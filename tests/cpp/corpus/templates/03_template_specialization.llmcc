Template specialization.
lang: cpp

--- file: src/lib.cpp ---
template<typename T>
class Wrapper {
    T data;
public:
    T get() { return data; }
};

template<>
class Wrapper<int> {
    int data;
public:
    int get() { return data * 2; }
};

template<typename T>
class Wrapper<T*> {
    T* ptr;
public:
    T get() { return *ptr; }
};

