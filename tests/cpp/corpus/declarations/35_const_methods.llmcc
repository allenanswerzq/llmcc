class Value {
    int data;
public:
    int get() const { return data; }
    void set(int v) { data = v; }

    int& at() { return data; }
    const int& at() const { return data; }

    Value& operator+=(int v) { data += v; return *this; }
    Value operator+(int v) const { return Value{data + v}; }
};

