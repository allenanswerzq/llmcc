Trailing return type syntax.
lang: cpp

--- file: src/lib.cpp ---
auto add(int a, int b) -> int {
    return a + b;
}

auto lambda = [](int x) -> double {
    return x * 1.5;
};

template<typename T, typename U>
auto multiply(T a, U b) -> decltype(a * b) {
    return a * b;
}

class Calculator {
public:
    auto compute(int x) -> int { return x * 2; }
};

