Empty base optimization patterns.
lang: cpp

--- file: src/lib.cpp ---
struct Empty {};

struct EmptyBase {};

struct Derived : EmptyBase {
    int value;
};

template<typename T>
struct Wrapper : T {
    int data;
};

struct HasEmpty {
    [[no_unique_address]] Empty e;
    int value;
};

