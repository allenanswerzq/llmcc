Constructors and destructors.
lang: cpp

--- file: src/lib.cpp ---
class Resource {
    int* data;
public:
    Resource() : data(nullptr) {}

    Resource(int size) : data(new int[size]) {}

    Resource(const Resource& other) = default;

    Resource(Resource&& other) noexcept : data(other.data) {
        other.data = nullptr;
    }

    ~Resource() {
        delete[] data;
    }
};

