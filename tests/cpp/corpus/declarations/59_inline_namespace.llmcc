namespace library {
    inline namespace v2 {
        class Widget {
        public:
            void newMethod() {}
        };
    }

    namespace v1 {
        class Widget {
        public:
            void oldMethod() {}
        };
    }
}

void use_library() {
    library::Widget w;
    w.newMethod();

    library::v1::Widget old;
    old.oldMethod();
}

