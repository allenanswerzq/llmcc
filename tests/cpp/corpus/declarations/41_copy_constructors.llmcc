class Widget {
    int* data;
public:
    Widget() : data(new int(0)) {}

    Widget(const Widget& other) : data(new int(*other.data)) {}

    Widget& operator=(const Widget& other) {
        if (this != &other) {
            delete data;
            data = new int(*other.data);
        }
        return *this;
    }

    ~Widget() { delete data; }
};

