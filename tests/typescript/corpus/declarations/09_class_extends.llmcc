class Base {
    id: number;

    constructor(id: number) {
        this.id = id;
    }
}

class Derived extends Base {
    name: string;

    constructor(id: number, name: string) {
        super(id);
        this.name = name;
    }
}

class Animal {
    move(): void {}
}

class Dog extends Animal {
    bark(): void {}
}

