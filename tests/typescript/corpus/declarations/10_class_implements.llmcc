--- file: src/lib.ts ---
interface Printable {
    print(): string;
}

interface Serializable {
    serialize(): string;
}

class Document implements Printable {
    title: string;

    print(): string {
        return this.title;
    }
}

class Report implements Printable, Serializable {
    data: string;

    print(): string {
        return this.data;
    }

    serialize(): string {
        return JSON.stringify(this.data);
    }
}

