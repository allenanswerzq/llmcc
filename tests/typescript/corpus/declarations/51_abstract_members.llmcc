abstract class Shape {
    abstract get area(): number;
    abstract draw(): void;
    
    describe(): string {
        return "A shape";
    }
}

class Circle extends Shape {
    constructor(public radius: number) { super(); }
    
    get area(): number { return Math.PI * this.radius ** 2; }
    draw(): void {}
}

