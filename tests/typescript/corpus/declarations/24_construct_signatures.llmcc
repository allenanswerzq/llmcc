Interface construct signatures.
lang: typescript

--- file: src/lib.ts ---
interface Constructor {
    new (): object;
}

interface TypedConstructor<T> {
    new (): T;
}

interface ParameterizedConstructor {
    new (name: string, age: number): Person;
}

interface GenericConstructor<T> {
    new (...args: any[]): T;
}

interface FactoryConstructor {
    new <T>(value: T): Container<T>;
}

interface Person {
    name: string;
    age: number;
}

interface Container<T> {
    value: T;
}

