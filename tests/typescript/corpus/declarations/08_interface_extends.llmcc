===============================================================================
interface-extends
===============================================================================
Interfaces extending other interfaces.
lang: typescript

--- file: src/lib.ts ---
interface Animal {
    name: string;
}

interface Mammal extends Animal {
    warmBlooded: boolean;
}

interface Dog extends Mammal {
    breed: string;
    bark(): void;
}

interface Named {
    name: string;
}

interface Aged {
    age: number;
}

interface Person extends Named, Aged {
    email: string;
}

