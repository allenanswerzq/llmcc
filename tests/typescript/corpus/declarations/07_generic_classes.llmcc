class Box<T> {
    private content: T;

    constructor(value: T) {
        this.content = value;
    }

    get(): T {
        return this.content;
    }
}

class Pair<T, U> {
    constructor(public first: T, public second: U) {}
}

class Repository<T extends { id: number }> {
    private items: T[];

    add(item: T): void {
        this.items.push(item);
    }
}

