class Person {
    public name: string;
    private age: number;
    protected email: string;
    readonly id: number;

    constructor(name: string, age: number) {
        this.name = name;
        this.age = age;
        this.id = Date.now();
    }

    public greet(): string {
        return `Hello, ${this.name}`;
    }

    private validate(): boolean {
        return this.age > 0;
    }

    protected getInfo(): string {
        return `${this.name}: ${this.age}`;
    }
}

