interface Config {
    required: string;
    optional?: number;
    readonly fixed: boolean;
}

interface User {
    name: string;
    email?: string;
    phone?: string;
}

function greet(name: string, greeting?: string): string {
    return `${greeting || "Hello"}, ${name}`;
}

function configure(host: string, port?: number, secure?: boolean): void {}

class Settings {
    theme?: string;
    fontSize?: number;

    constructor(theme?: string) {
        this.theme = theme;
    }
}

