Type narrowing with control flow.
lang: typescript

--- file: src/lib.ts ---
function process(value: string | number | null): string {
    if (value === null) {
        return "null";
    }
    if (typeof value === "string") {
        return value.toUpperCase();
    }
    return value.toFixed(2);
}

