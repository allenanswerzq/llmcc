interface Todo {
    title: string;
    description: string;
    completed: boolean;
}

type PartialTodo = Partial<Todo>;
type RequiredTodo = Required<Todo>;
type PickTitle = Pick<Todo, "title">;
type OmitDesc = Omit<Todo, "description">;
type RecordType = Record<string, number>;

