--- file: src/lib.ts ---
type Container<T> = { value: T };
type Pair<T, U> = { first: T; second: U };
type Wrapper<T> = T extends null ? never : T;

interface Box<T> {
    contents: T;
    isEmpty(): boolean;
}

interface Map<K, V> {
    get(key: K): V | undefined;
    set(key: K, value: V): void;
}

class Stack<T> {
    private items: T[] = [];

    push(item: T): void {
        this.items.push(item);
    }

    pop(): T | undefined {
        return this.items.pop();
    }
}

