Intersection type declarations.
lang: typescript

--- file: src/lib.ts ---
type Named = { name: string };
type Aged = { age: number };
type Person = Named & Aged;

type Serializable = { serialize(): string };
type Comparable = { compare(other: any): number };
type Entity = Serializable & Comparable;

interface A {
    a: string;
}

interface B {
    b: number;
}

type AB = A & B;

function merge<T, U>(obj1: T, obj2: U): T & U {
    return { ...obj1, ...obj2 };
}

