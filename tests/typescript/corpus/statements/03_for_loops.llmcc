function sum(n: number): number {
    let total = 0;
    for (let i = 0; i < n; i++) {
        total += i;
    }
    return total;
}

function process(items: string[]): void {
    for (const item of items) {
        console.log(item);
    }
}

function keys(obj: object): void {
    for (const key in obj) {
        console.log(key);
    }
}

