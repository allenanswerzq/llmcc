===============================================================================
deeply-nested-namespace
===============================================================================
Test that deeply nested namespaces (A.B.C.Type) resolve correctly.
lang: typescript

--- file: sample/src/lib.ts ---

namespace Api {
    export namespace V1 {
        export namespace Models {
            export class User {
                id: number;
                name: string;
            }

            export interface IEntity {
                getId(): number;
            }
        }
    }
}

namespace Services {
    export class UserService {
        // Use deeply nested path: Api.V1.Models.User
        getUser(id: number): Api.V1.Models.User {
            return new Api.V1.Models.User();
        }

        // Use deeply nested interface
        getEntity(): Api.V1.Models.IEntity {
            throw new Error("not implemented");
        }
    }
}

