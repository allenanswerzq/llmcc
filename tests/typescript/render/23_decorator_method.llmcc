function Log() {
    return function(target: any, key: string, descriptor: PropertyDescriptor) {
        return descriptor;
    };
}

function Validate() {
    return function(target: any, key: string, descriptor: PropertyDescriptor) {
        return descriptor;
    };
}

class UserService {
    @Log()
    @Validate()
    createUser(name: string): void {
        console.log(name);
    }
}

