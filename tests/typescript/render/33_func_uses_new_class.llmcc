===============================================================================
func-uses-new-class
===============================================================================
Test that free functions using new Class() create type dependency edges.
lang: typescript

--- file: sample/src/lib.ts ---

export class User {
    id: number;
    name: string;

    initialize(): void {
        this.id = 0;
        this.name = "";
    }
}

export class Order {
    orderId: number;
    amount: number;
}

export function createUser(): User {
    const user = new User();
    user.initialize();
    return user;
}

export function createOrder(): Order {
    const order = new Order();
    return order;
}

export function createBoth(): User {
    const user = new User();
    const order = new Order();
    return user;
}

