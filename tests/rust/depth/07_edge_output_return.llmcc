===============================================================================
edge-output-return
===============================================================================
Test that output edges create correct dependency direction.
When function F returns type T, F's crate depends on T's crate.
At crate level: crate_app -> crate_types (app depends on types)

--- file: types/Cargo.toml ---
[package]
name = "types"

--- file: types/src/lib.rs ---
pub struct Response {
    pub status: u32,
}

--- file: app/Cargo.toml ---
[package]
name = "app"

[dependencies]
types = { path = "../types" }

--- file: app/src/lib.rs ---
use types::Response;

/// Function returns Response type
pub fn create_response() -> Response {
    Response { status: 200 }
}

--- expect:arch-graph-depth-3 ---
digraph architecture {
  rankdir=TB;
  ranksep=0.6;
  nodesep=0.3;
  splines=ortho;
  compound=true;

  node [shape=box, style=rounded];
  edge [arrowsize=0.7];

  subgraph cluster_app {
    label="app";
    bgcolor="#f8f8f8";

    subgraph cluster_lib_rs {
      label="lib.rs";
      bgcolor="#fafafa";

      n5[label="create_response", full_path="$TMP/app/src/lib.rs:4", sym_ty="Function", shape=ellipse];
    }

  }

  subgraph cluster_types {
    label="types";
    bgcolor="#f8f8f8";

    subgraph cluster_lib_rs {
      label="lib.rs";
      bgcolor="#fafafa";

      n2[label="Response", full_path="$TMP/types/src/lib.rs:1", sym_ty="Struct", shape=box];
    }

  }


  n5 -> n2 [from="func", to="output"];
}

