===============================================================================
two-crates-basic
===============================================================================
Basic test with two crates where one depends on the other.

--- file: Cargo.toml ---
[workspace]
members = ["common", "app"]

--- file: common/Cargo.toml ---
[package]
name = "common"
version = "0.1.0"

--- file: common/src/lib.rs ---
pub mod types;

--- file: common/src/types.rs ---
pub struct User {
    pub id: u64,
    pub name: String,
}

pub struct Error {
    pub code: i32,
    pub message: String,
}

--- file: app/Cargo.toml ---
[package]
name = "app"
version = "0.1.0"

[dependencies]
common = { path = "../common" }

--- file: app/src/lib.rs ---
pub mod service;

--- file: app/src/service.rs ---
use common::types::{User, Error};

pub fn get_user(id: u64) -> Result<User, Error> {
    Ok(User { id, name: String::new() })
}

pub fn create_user(name: String) -> User {
    User { id: 1, name }
}

