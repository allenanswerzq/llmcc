use eval::Evaluator;

fn main() {
    // (1 + 2) * 3
    let expr = Expr::Binary {
        op: BinOp::Mul,
        left: Box::new(Expr::Binary {
            op: BinOp::Add,
            left: Box::new(Expr::Number(1)),
            right: Box::new(Expr::Number(2)),
        }),
        right: Box::new(Expr::Number(3)),
    };

    let mut eval = Evaluator;
    println!("Result: {}", eval.visit(&expr));
}

