mod models;
mod services;
mod utils;

use services::UserService;
use utils::validate_user;

fn main() {
    let mut service = UserService::new();
    let user = service.create_user("Alice".to_string());

    if validate_user(user) {
        println!("User {} is valid", user.name);
    }
}

