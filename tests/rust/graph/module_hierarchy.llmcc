pub struct Email(String);

impl Email {
    pub fn new(email: String) -> Option<Self> {
        if email.contains('@') {
            Some(Email(email))
        } else {
            None
        }
    }

    pub fn as_str(&self) -> &str {
        &self.0
    }
}

