use super::profile::Profile;

pub struct User {
    pub id: u64,
    pub name: String,
    pub profile: Option<Profile>,
}

impl User {
    pub fn new(id: u64, name: String) -> Self {
        User { id, name, profile: None }
    }

    pub fn with_profile(mut self, profile: Profile) -> Self {
        self.profile = Some(profile);
        self
    }
}

