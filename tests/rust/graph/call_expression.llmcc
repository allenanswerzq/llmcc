--- file: app/src/main.rs ---
struct Foo;
struct Baz;
struct SessionBuilder;

impl SessionBuilder {
    fn new() -> Self {
        SessionBuilder
    }
    
    fn static_func() -> Foo {}
}

fn qualified_call(baz: &Baz) {
    SessionBuilder::new();
    let x = SessionBuilder::static_func();
}

