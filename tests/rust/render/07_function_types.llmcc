===============================================================================
function-params-return
===============================================================================
Test that function parameter and return types create edges.

--- file: sample/Cargo.toml ---
[package]
name = "sample"

--- file: sample/src/lib.rs ---


pub struct Input {
    pub data: String,
}

pub struct Output {
    pub result: i32,
}

pub fn transform(input: Input) -> Output {
    Output { result: input.data.len() as i32 }
}

--- expect:arch-graph ---
digraph architecture {
  rankdir=TB;
  ranksep=0.6;
  nodesep=0.3;
  splines=ortho;
  compound=true;

  node [shape=box, style=rounded];
  edge [arrowsize=0.7];

  subgraph cluster_sample {
    label="sample";
    style=rounded;
        bgcolor="#f8f8f8";

    subgraph cluster_lib_rs {
      label="lib.rs";
      style=rounded;
            bgcolor="#fafafa";

      n6[label="transform", path="$TMP/sample/src/lib.rs:11", sym_ty="Function", shape=ellipse];
      n2[label="Input", path="$TMP/sample/src/lib.rs:3", sym_ty="Struct", shape=box];
      n4[label="Output", path="$TMP/sample/src/lib.rs:7", sym_ty="Struct", shape=box];
    }

  }


  n2 -> n6 [from="input", to="func"];
  n6 -> n4 [from="func", to="output"];
}
