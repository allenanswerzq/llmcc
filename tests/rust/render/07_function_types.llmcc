===============================================================================
function-params-return
===============================================================================
Test that function parameter and return types create edges.

--- file: sample/Cargo.toml ---
[package]
name = "sample"

--- file: sample/src/lib.rs ---


pub struct Input {
    pub data: String,
}

pub struct Output {
    pub result: i32,
}

pub fn transform(input: Input) -> Output {
    Output { result: input.data.len() as i32 }
}

--- expect:arch-graph ---
digraph architecture {
  subgraph cluster_project {
    label="project";

    subgraph cluster_sample {
      label="sample";
      subgraph cluster_lib_rs {
        label="lib.rs";
        n6[label="transform", full_path="$TMP/sample/src/lib.rs:11", sym_ty="Function"];
        n2[label="Input", full_path="$TMP/sample/src/lib.rs:3", sym_ty="Struct", shape=box];
        n4[label="Output", full_path="$TMP/sample/src/lib.rs:7", sym_ty="Struct", shape=box];
      }

    }

  }

  n2 -> n6 [from="input", to="func"];
  n6 -> n4 [from="func", to="output"];
}
