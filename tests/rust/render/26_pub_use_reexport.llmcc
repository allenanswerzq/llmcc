(root:1 lib ($TMP/sample/src/lib.rs))

(root:2 internal ($TMP/sample/src/internal.rs)
  (class:3 Secret
    (field:4 key @type String)
  )
)

(root:5 consumer ($TMP/sample/src/consumer.rs)
  (class:6 Consumer
    (field:7 secret @type:3 Secret)
  )
)

