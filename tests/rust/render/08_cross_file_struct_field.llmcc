===============================================================================
cross-file-struct-field
===============================================================================
Struct in one file uses struct from another file as field type.

--- file: sample/Cargo.toml ---
[package]
name = "sample"

--- file: sample/src/lib.rs ---
mod model;
mod container;

--- file: sample/src/model.rs ---
pub struct Item {
    pub id: u64,
}

--- file: sample/src/container.rs ---
use crate::model::Item;

pub struct Box {
    pub item: Item,
}

--- expect:block-graph ---
(root:1 lib ($TMP/sample/src/lib.rs))

(root:2 model ($TMP/sample/src/model.rs)
  (class:3 Item
    (field:4 id @type u64)
  )
)

(root:5 container ($TMP/sample/src/container.rs)
  (class:6 Box
    (field:7 item @type:3 Item)
  )
)

--- expect:arch-graph ---
digraph architecture {
  subgraph cluster_project {
    label="project";

    subgraph cluster_sample {
      label="sample";
      subgraph cluster_container_rs {
        label="container.rs";
        n6[label="Box", full_path="$TMP/sample/src/container.rs:3", sym_ty="Struct", shape=box];
      }

      subgraph cluster_model_rs {
        label="model.rs";
        n3[label="Item", full_path="$TMP/sample/src/model.rs:1", sym_ty="Struct", shape=box];
      }

    }

  }

  n3 -> n6 [from="field_type", to="struct"];
}
