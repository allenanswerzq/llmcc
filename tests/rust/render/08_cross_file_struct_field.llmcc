===============================================================================
cross-file-struct-field
===============================================================================
Struct in one file uses struct from another file as field type.

--- file: sample/Cargo.toml ---
[package]
name = "sample"

--- file: sample/src/lib.rs ---
mod model;
mod container;

--- file: sample/src/model.rs ---
pub struct Item {
    pub id: u64,
}

--- file: sample/src/container.rs ---
use crate::model::Item;

pub struct Box {
    pub item: Item,
}

--- expect:block-graph ---
(root:1 lib ($TMP/sample/src/lib.rs))

(root:2 model ($TMP/sample/src/model.rs)
  (class:3 Item
    (field:4 id @type u64)
  )
)

(root:5 container ($TMP/sample/src/container.rs)
  (class:6 Box
    (field:7 item @type:3 Item)
  )
)

--- expect:arch-graph ---
digraph architecture {
  // Layout settings
  rankdir=TB;
