===============================================================================
cross-file-struct-field
===============================================================================
Struct in one file uses struct from another file as field type.

--- file: sample/Cargo.toml ---
[package]
name = "sample"

--- file: sample/src/lib.rs ---
mod model;
mod container;

--- file: sample/src/model.rs ---
pub struct Item {
    pub id: u64,
}

--- file: sample/src/container.rs ---
use crate::model::Item;

pub struct Box {
    pub item: Item,
}

--- expect:block-graph ---
(root:1 lib ($TMP/sample/src/lib.rs)
  (module:2 model)
  (module:3 container)
)

(root:4 model ($TMP/sample/src/model.rs)
  (class:5 Item
    (field:6 id @type u64)
  )
)

(root:7 container ($TMP/sample/src/container.rs)
  (class:8 Box
    (field:9 item @type:5 Item)
    (@tdep:5 Item)
  )
)

--- expect:arch-graph ---
digraph architecture {
  rankdir=TB;
  ranksep=0.6;
  nodesep=0.3;
  splines=ortho;
  compound=true;

  node [shape=box, style=rounded];
  edge [arrowsize=0.7];

  subgraph cluster_sample {
    label="sample";
    style=rounded;
        bgcolor="#f8f8f8";

    subgraph cluster_container_rs {
      label="container.rs";
      style=rounded;
            bgcolor="#fafafa";

      n8[label="Box", path="$TMP/sample/src/container.rs:3", sym_ty="Struct", shape=box];
    }

    subgraph cluster_model_rs {
      label="model.rs";
      style=rounded;
            bgcolor="#fafafa";

      n5[label="Item", path="$TMP/sample/src/model.rs:1", sym_ty="Struct", shape=box];
    }

  }


  n5 -> n8 [from="field_type", to="struct"];
}
