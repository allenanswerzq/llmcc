===============================================================================
trait-and-impl
===============================================================================
Test that traits and their implementations create edges.

--- file: sample/Cargo.toml ---
[package]
name = "sample"

--- file: sample/src/lib.rs ---

pub trait Printable {
    fn print(&self);
}

pub struct Document {
    pub content: String,
}

impl Printable for Document {
    fn print(&self) {
        println!("{}", self.content);
    }
}

--- expect:arch-graph ---
digraph architecture {
  subgraph cluster_project {
    label="project";

    subgraph cluster_sample {
      label="sample";
      subgraph cluster_lib_rs {
        label="lib.rs";
        n2[label="Printable", full_path="$TMP/sample/src/lib.rs:2", sym_ty="Trait", shape=box];
        n5[label="Document", full_path="$TMP/sample/src/lib.rs:6", sym_ty="Struct", shape=box];
      }

    }

  }

  n2 -> n5 [from="trait", to="impl"];
}
