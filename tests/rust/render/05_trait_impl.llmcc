===============================================================================
trait-and-impl
===============================================================================
Test that traits and their implementations create edges.

--- file: sample/Cargo.toml ---
[package]
name = "sample"

--- file: sample/src/lib.rs ---

pub trait Printable {
    fn print(&self);
}

pub struct Document {
    pub content: String,
}

impl Printable for Document {
    fn print(&self) {
        println!("{}", self.content);
    }
}

--- expect:arch-graph ---
digraph architecture {
  rankdir=TB;
  ranksep=0.6;
  nodesep=0.3;
  splines=ortho;
  compound=true;

  node [shape=box, style=rounded];
  edge [arrowsize=0.7];

  subgraph cluster_sample {
    label="sample";
    style=rounded;
        bgcolor="#f8f8f8";

    subgraph cluster_lib_rs {
      label="lib.rs";
      style=rounded;
            bgcolor="#fafafa";

      n2[label="Printable", path="$TMP/sample/src/lib.rs:2", sym_ty="Trait", shape=box];
      n5[label="Document", path="$TMP/sample/src/lib.rs:6", sym_ty="Struct", shape=box];
    }

  }


  n2 -> n5 [from="trait", to="impl"];
}
