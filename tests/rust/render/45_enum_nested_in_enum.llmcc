===============================================================================
enum-nested-in-enum
===============================================================================
Test enums containing other enums in variants.

--- file: sample/Cargo.toml ---
[package]
name = "sample"

--- file: sample/src/lib.rs ---
mod types;
mod messages;

--- file: sample/src/types.rs ---
pub enum Priority {
    Low,
    Medium,
    High,
}

pub enum Status {
    Pending,
    Active,
    Done,
}

--- file: sample/src/messages.rs ---
use crate::types::{Priority, Status};

pub enum Message {
    Simple(String),
    StatusUpdate {
        status: Status,
        priority: Priority,
    },
    PriorityChange(Priority),
}

pub enum Notification {
    Message(Message),
    Alert {
        priority: Priority,
    },
}

