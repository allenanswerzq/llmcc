===============================================================================
cross-file-func-param
===============================================================================
Function in one file takes struct from another file as parameter.

--- file: sample/Cargo.toml ---
[package]
name = "sample"

--- file: sample/src/lib.rs ---
mod types;
mod handler;

--- file: sample/src/types.rs ---
pub struct Request {
    pub body: String,
}

--- file: sample/src/handler.rs ---
use crate::types::Request;

pub fn handle(req: Request) {
    println!("{}", req.body);
}

--- expect:block-graph ---
(root:1 lib ($TMP/sample/src/lib.rs))

(root:2 types ($TMP/sample/src/types.rs)
  (class:3 Request
    (field:4 body @type String)
  )
)

(root:5 handler ($TMP/sample/src/handler.rs)
  (func:6 handle
    (parameter:7 req @type:3 Request)
  )
)

