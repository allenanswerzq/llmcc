===============================================================================
simple-struct
===============================================================================
Test that a simple struct appears in the architecture graph.

--- file: sample/Cargo.toml ---
[package]
name = "sample"


--- file: sample/src/lib.rs ---

struct Foo;

pub struct User {
    pub name: Foo,
    pub age: i32,
}

--- expect:arch-graph ---
digraph architecture {
  // Layout settings
  rankdir=TB;
  ranksep=0.6;
  nodesep=0.3;
  splines=ortho;
  compound=true;

  node [shape=box, style=rounded];
  edge [arrowsize=0.7];

  subgraph cluster_sample {
    label="sample";
    style=rounded;
    color="#888888";
    bgcolor="#f0f0f0";

    subgraph cluster_lib_rs {
      label="lib.rs";
      style=rounded;
      color="#aaaaaa";
      bgcolor="#fafafa";

      n2[label="Foo", full_path="$TMP/sample/src/lib.rs:2", sym_ty="Struct", shape=box];
      n3[label="User", full_path="$TMP/sample/src/lib.rs:4", sym_ty="Struct", shape=box];
    }

  }


  n2 -> n3 [from="field_type", to="struct"];
  n2 -> n3 [from="interface", to="implements"];
}
