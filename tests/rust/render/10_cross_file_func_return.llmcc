===============================================================================
cross-file-func-return
===============================================================================
Function in one file returns struct from another file.

--- file: sample/Cargo.toml ---
[package]
name = "sample"

--- file: sample/src/lib.rs ---
mod types;
mod factory;

--- file: sample/src/types.rs ---
pub struct Config {
    pub name: String,
}

--- file: sample/src/factory.rs ---
use crate::types::Config;

pub fn create_config() -> Config {
    Config { name: "default".to_string() }
}

--- expect:block-graph ---
(root:1 lib ($TMP/sample/src/lib.rs))

(root:2 types ($TMP/sample/src/types.rs)
  (class:3 Config
    (field:4 name @type String)
  )
)

(root:5 factory ($TMP/sample/src/factory.rs)
  (func:6 create_config
    (return:7 @type:3 Config)
  )
)

