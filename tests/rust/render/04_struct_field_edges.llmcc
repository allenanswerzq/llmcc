===============================================================================
struct-field-edges
===============================================================================
Test that edges are drawn from structs to their field types.

--- file: sample/Cargo.toml ---
[package]
name = "sample"

--- file: sample/src/lib.rs ---

pub struct Address {
    pub street: String,
    pub city: String,
}

pub struct Person {
    pub name: String,
    pub home: Address,
}

--- expect:arch-graph ---
digraph architecture {
  subgraph cluster_project {
    label="project";

    subgraph cluster_sample {
      label="sample";
      subgraph cluster_lib_rs {
        label="lib.rs";
        n2[label="Address", full_path="$TMP/sample/src/lib.rs:2", sym_ty="Struct", shape=box];
        n5[label="Person", full_path="$TMP/sample/src/lib.rs:7", sym_ty="Struct", shape=box];
      }

    }

  }

  n2 -> n5 [from="field_type", to="struct"];
}
