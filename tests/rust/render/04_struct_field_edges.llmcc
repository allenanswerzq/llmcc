===============================================================================
struct-field-edges
===============================================================================
Test that edges are drawn from structs to their field types.

--- file: sample/Cargo.toml ---
[package]
name = "sample"

--- file: sample/src/lib.rs ---

pub struct Address {
    pub street: String,
    pub city: String,
}

pub struct Person {
    pub name: String,
    pub home: Address,
}

--- expect:arch-graph ---
digraph architecture {
  rankdir=TB;
  ranksep=0.6;
  nodesep=0.3;
  splines=ortho;
  compound=true;

  node [shape=box, style=rounded];
  edge [arrowsize=0.7];

  subgraph cluster_sample {
    label="sample";
    style=rounded;
        bgcolor="#f8f8f8";

    subgraph cluster_lib_rs {
      label="lib.rs";
      style=rounded;
            bgcolor="#fafafa";

      n2[label="Address", path="$TMP/sample/src/lib.rs:2", sym_ty="Struct", shape=box];
      n5[label="Person", path="$TMP/sample/src/lib.rs:7", sym_ty="Struct", shape=box];
    }

  }


  n2 -> n5 [from="field_type", to="struct"];
}
