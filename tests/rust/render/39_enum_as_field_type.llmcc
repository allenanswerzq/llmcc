===============================================================================
enum-as-field-type
===============================================================================
Test enums used as field types in structs.

--- file: sample/Cargo.toml ---
[package]
name = "sample"

--- file: sample/src/lib.rs ---
mod types;
mod models;

--- file: sample/src/types.rs ---
pub enum Status {
    Active,
    Inactive,
    Pending,
}

pub enum Priority {
    Low,
    Medium,
    High,
}

--- file: sample/src/models.rs ---
use crate::types::{Status, Priority};

pub struct Task {
    pub name: String,
    pub status: Status,
    pub priority: Priority,
}

pub struct Project {
    pub title: String,
    pub status: Status,
}

