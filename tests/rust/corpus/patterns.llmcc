    name: String,
    age: u32,
}

enum Container {
    Item(Option<i32>),
}

fn greet(person: Person) -> &'static str {
    match person {
        Person { name, age } if age < 5 => "toddler",
        Person { name: _, age: _ } => "adult",
    }
}

fn extract_value(container: Container) -> Option<i32> {
    match container {
        Container::Item(Some(x)) => Some(x),
        Container::Item(None) => None,
    }
}

