--- file: src/lib.rs ---

struct Foo;
struct Bar {
    a: i32,
}
impl Bar {
    fn new(a: i32) -> Self {
        Self { a }
    }

    fn get_a(&self) -> i32 {
        self.a
    }

    fn nothing() {
    }
}

struct ComplexType<T> {
    y: Foo,
};


fn free() {}

fn add(x: i32, t: ComplexType<T>) -> i32 {
    if x > 0 {
        println!("{}", x);
    } else {
        let b = Bar::new(x);
        println!("{}", b.get_a());
    }
    free();
    return x + y;
}

