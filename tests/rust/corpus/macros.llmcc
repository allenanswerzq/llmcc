--- file: src/main.rs ---
macro_rules! a [];
fn foo() {
    a!(* a *);
    a!(& a &);
    a!(- a -);
    a!(b + c + +);
    a!('a'..='z');
    a!('\u{0}'..='\u{2}');
    a!('lifetime)
    default!(a);
    union!(a);
    a!($);
    a!($());
    a!($ a $);
    a!(${$([ a ])});
    a!($a $a:ident $($a);*);
}

