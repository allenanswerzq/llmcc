digraph architecture {
  // Layout settings
  rankdir=TB;
  ranksep=0.8;
  nodesep=0.4;
  splines=ortho;
  concentrate=true;

  node [shape=box, style="rounded,filled", fillcolor="#f0f0f0", fontname="Helvetica"];
  edge [color="#666666", arrowsize=0.7];

  labelloc=t;
  fontsize=16;
  fontname="Helvetica Bold";

  crate_lancedb[label="lancedb", folder="/home/yibai/llmcc/sample/repos/lancedb/rust/lancedb"];
  crate_lancedb_nodejs[label="lancedb-nodejs", folder="/home/yibai/llmcc/sample/repos/lancedb/nodejs"];
  crate_lancedb_python[label="lancedb-python", folder="/home/yibai/llmcc/sample/repos/lancedb/python"];

  crate_lancedb -> crate_lancedb_nodejs;
  crate_lancedb -> crate_lancedb_python;
  crate_lancedb_nodejs -> crate_lancedb;
  crate_lancedb_nodejs -> crate_lancedb_python;
  crate_lancedb_python -> crate_lancedb;
  crate_lancedb_python -> crate_lancedb_nodejs;
}
