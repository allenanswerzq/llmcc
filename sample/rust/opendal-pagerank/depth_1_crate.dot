digraph architecture {
  // Layout settings
  rankdir=TB;
  ranksep=0.8;
  nodesep=0.4;
  splines=ortho;
  concentrate=true;

  node [shape=box, style="rounded,filled", fillcolor="#f0f0f0", fontname="Helvetica"];
  edge [color="#666666", arrowsize=0.7];

  labelloc=t;
  fontsize=16;
  fontname="Helvetica Bold";

  crate_opendal_core[label="opendal-core"];
  crate_opendal_hs[label="opendal-hs"];
  crate_opendal_java[label="opendal-java"];
  crate_opendal_nodejs[label="opendal-nodejs"];
  crate_opendal_ocaml[label="opendal-ocaml"];
  crate_opendal_python[label="opendal-python"];
  crate_opendal_service_aliyun_drive[label="opendal-service-aliyun-drive"];
  crate_opendal_service_b2[label="opendal-service-b2"];
  crate_opendal_service_memcached[label="opendal-service-memcached"];
  crate_opendal_service_oss[label="opendal-service-oss"];
  crate_opendal_service_s3[label="opendal-service-s3"];

  crate_opendal_core -> crate_opendal_hs;
  crate_opendal_core -> crate_opendal_nodejs [dir=both]; // dir best-effort not accurate
  crate_opendal_core -> crate_opendal_python [dir=both]; // dir best-effort not accurate
  crate_opendal_java -> crate_opendal_python;
  crate_opendal_ocaml -> crate_opendal_nodejs;
  crate_opendal_ocaml -> crate_opendal_python;
  crate_opendal_service_aliyun_drive -> crate_opendal_core;
  crate_opendal_service_b2 -> crate_opendal_core;
  crate_opendal_service_memcached -> crate_opendal_core;
  crate_opendal_service_oss -> crate_opendal_core;
  crate_opendal_service_s3 -> crate_opendal_core;
}
