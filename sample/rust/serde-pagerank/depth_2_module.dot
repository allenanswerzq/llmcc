digraph architecture {
  // Layout settings
  rankdir=TB;
  ranksep=0.8;
  nodesep=0.4;
  splines=ortho;
  concentrate=true;

  node [shape=box, style="rounded,filled", fillcolor="#f0f0f0", fontname="Helvetica"];
  edge [color="#666666", arrowsize=0.7];

  labelloc=t;
  fontsize=16;
  fontname="Helvetica Bold";

  mod_serde_core_de[label="serde_core::de", folder="/home/yibai/llmcc/sample/repos/serde/serde_core/src/de"];
  mod_serde_derive_de[label="serde_derive::de", folder="/home/yibai/llmcc/sample/repos/serde/serde_derive/src"];
  mod_serde_derive_fragment[label="serde_derive::fragment", folder="/home/yibai/llmcc/sample/repos/serde/serde_derive/src"];
  mod_serde_derive_internals[label="serde_derive::internals", folder="/home/yibai/llmcc/sample/repos/serde/serde_derive/src/internals"];
  mod_serde_private[label="serde::private", folder="/home/yibai/llmcc/sample/repos/serde/serde/src/private"];

  mod_serde_derive_de -> mod_serde_derive_fragment;
  mod_serde_derive_de -> mod_serde_derive_internals;
  mod_serde_private -> mod_serde_core_de;
}
