digraph architecture {
  // Layout settings
  rankdir=TB;
  ranksep=0.8;
  nodesep=0.4;
  splines=ortho;
  concentrate=true;

  node [shape=box, style="rounded,filled", fillcolor="#f0f0f0", fontname="Helvetica"];
  edge [color="#666666", arrowsize=0.7];

  labelloc=t;
  fontsize=16;
  fontname="Helvetica Bold";

  crate_clap_builder[label="clap_builder", folder="/home/yibai/llmcc/sample/repos/clap/clap_builder"];
  crate_clap_complete[label="clap_complete", folder="/home/yibai/llmcc/sample/repos/clap/clap_complete"];
  crate_clap_complete_nushell[label="clap_complete_nushell", folder="/home/yibai/llmcc/sample/repos/clap/clap_complete_nushell"];
  crate_clap_lex[label="clap_lex", folder="/home/yibai/llmcc/sample/repos/clap/clap_lex"];
  crate_clap_mangen[label="clap_mangen", folder="/home/yibai/llmcc/sample/repos/clap/clap_mangen"];

  crate_clap_complete -> crate_clap_builder;
  crate_clap_complete_nushell -> crate_clap_builder;
  crate_clap_lex -> crate_clap_builder;
  crate_clap_mangen -> crate_clap_builder;
}
