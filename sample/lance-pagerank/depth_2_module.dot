  edge [color="#666666", arrowsize=0.7];

  label="module graph";
  labelloc=t;
  fontsize=16;
  fontname="Helvetica Bold";

  subgraph cluster_lance {
    label="lance";
    style=rounded;
    color="#888888";
    bgcolor="#f0f0f0";
    fontname="Helvetica Bold";

