digraph architecture {
  // Layout settings
  rankdir=TB;
  ranksep=0.8;
  nodesep=0.4;
  splines=ortho;
  concentrate=true;

  node [shape=box, style="rounded,filled", fillcolor="#f0f0f0", fontname="Helvetica"];
  edge [color="#666666", arrowsize=0.7];

  labelloc=t;
  fontsize=16;
  fontname="Helvetica Bold";

  subgraph cluster_shadcn {
    label="shadcn";
    style=rounded;
    color="#888888";
    bgcolor="#f0f0f0";
    fontname="Helvetica Bold";

    mod_shadcn_commands[label="commands", folder="/home/yibai/llmcc/sample/repos/shadcn-ui/packages/shadcn/src/commands"];
    mod_shadcn_migrations[label="migrations", folder="/home/yibai/llmcc/sample/repos/shadcn-ui/packages/shadcn/src/migrations"];
    mod_shadcn_preflights[label="preflights", folder="/home/yibai/llmcc/sample/repos/shadcn-ui/packages/shadcn/src/preflights"];
    mod_shadcn_registry[label="registry", folder="/home/yibai/llmcc/sample/repos/shadcn-ui/packages/shadcn/src/registry"];
    mod_shadcn_utils[label="utils", folder="/home/yibai/llmcc/sample/repos/shadcn-ui/packages/shadcn/src/utils"];
  }


  mod_shadcn_commands -> mod_shadcn_registry;
  mod_shadcn_commands -> mod_shadcn_utils;
  mod_shadcn_migrations -> mod_shadcn_utils;
  mod_shadcn_preflights -> mod_shadcn_utils;
  mod_shadcn_registry -> mod_shadcn_utils;
  mod_shadcn_utils -> mod_shadcn_registry;
}
