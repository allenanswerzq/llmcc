  edge [color="#666666", arrowsize=0.7];

  labelloc=t;
  fontsize=16;
  fontname="Helvetica Bold";

  crate_shadcn[label="shadcn", folder="/home/yibai/llmcc/sample/repos/shadcn-ui/packages/shadcn"];
  crate_v4[label="v4", folder="/home/yibai/llmcc/sample/repos/shadcn-ui/apps/v4"];
  crate_www[label="www", folder="/home/yibai/llmcc/sample/repos/shadcn-ui/deprecated/www"];

  crate_v4 -> crate_shadcn;
  crate_v4 -> crate_www;
  crate_www -> crate_v4;
